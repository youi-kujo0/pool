int	much_words(char c, char *charset)
{
	int	i;
	i = 0;
      	while(charset[i])
	{
		if(c == charset)
			return 1;
		i++;
	}
	return 0;
}

/*char **/ int	ft_split(char *str, char *charset)
{
	int	i;
	char	**arr;
	int	count_words;
	int	word;

	word = 0;
	i = 0;
	if(much_words(str[i], charset))
	return count_words;
}
#include <stdio.h>
int main()
{
	printf("%d",ft_split("hello-world", "-"));

}
